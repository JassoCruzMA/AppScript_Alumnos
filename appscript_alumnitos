 * ðŸ§¹ LIMPIAR CAMPOS DE LOGIN
 * Limpia los campos de usuario y contraseÃ±a
 */
function limpiarCamposLogin() {
  const ss = SpreadsheetApp.getActiveSpreadsheet();
  const hoja = ss.getActiveSheet();
  
  hoja.getRange(CONFIG_CELDAS.USUARIO_INPUT).clearContent();
  hoja.getRange(CONFIG_CELDAS.CONTRASEÃ‘A_INPUT).clearContent();
}

